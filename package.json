  "nyc": {
    "check-coverage": true,
    "include": [
      "dist/src/**/*.js"
    ],
    "exclude": [
      "dist/src/index.js",
      "dist/test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
