{
  "functions": {
    "source": "dist"
  },
  "hosting": {
    "public": "dist-web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "app"
      }, {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
